<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.xstream"
	name="xStream"
	version="2.0.16"
	provider-name="MaxMustermann">
  <requires>
    <import addon="script.module.urlresolver" version="1.0.0" />
	<import addon="script.module.t0mm0.common" version="1.1.0" />
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="de">xStream</summary>
    <description lang="de">xStream ist ein Plugin, welches mit eigenen Plugins erweitert werden kann. Hiermit kann man Filme, Serien, etc. per Stream anschauen</description>
    <summary lang="en">xStream</summary>
    <description lang="en">Plugin to watch streams from various film an series sites.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="xbmc.repo.monkeemedia.ch"
        name="monkeemedia.ch repository"
        version="0.0.3"
        provider-name="monkeemedia.ch">
    <extension point="xbmc.addon.repository"
        name="monkeemedia.ch Add-on Repository">
        <info>http://content.wuala.com/contents/autobaderborn/xbmc.de/addons/xbmc.repo.xbmc.de/addons.xml?dl=1</info>
		<checksum>http://content.wuala.com/contents/autobaderborn/xbmc.de/addons/xbmc.repo.xbmc.de/addons.xml.md5?dl=1</checksum>
		<datadir zip="false">https://github.com/monkee-ch/xbmc-addon-sfvideoportal.git</datadir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>monkeemedia.cd Add-ons for XBMC</summary>
        <description>Download and install Add-ons for XBMC.</description>
        <disclaimer>No Risk! No Fun!</disclaimer>
        <platform>all</platform>
    </extension>
</addon>
</addons>
